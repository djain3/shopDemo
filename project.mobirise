                    "&:nth-child(even)": {
                      ".card-wrapper": {
                        "flex-direction": "row-reverse"
                      }
                    }
                  }
                },
                "& when (@reverseContent)": {
                  "@media (min-width: 768px)": {
                    "&:nth-child(odd)": {
                      ".card-wrapper": {
                        "flex-direction": "row-reverse"
                      }
                    }
                  }
                }
              }
            },
            ".card-wrapper": {
              "width": "100%",
              "height": "100%",
              "& when (@showBorders)": {
