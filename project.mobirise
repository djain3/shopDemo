ted condition=\"(blockType==1) || (blockType==3) || blockType==7\">\n\t<input type=\"range\" inline title=\"Icons Count\" name=\"iconsCount\" min=\"1\" max=\"10\" value=\"4\" step=\"1\" condition=\"blockType==1 || blockType==3 || blockType==7\">\n\t<!--  -->\n\t<fieldset type=\"background\" name=\"bg\">\n\t\t<input type=\"image\" title=\"Background Image\" value=\"../_images/background4.jpg\">\n\t\t<input type=\"color\" title=\"Background Color\" value=\"#d0d7a6\" selected>\n\t</fieldset>\n\t<!--  -->\n\t<!--  -->\n\t</mbr-parameters>\n\t<!--  -->\n\t<div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n\t\t<div class=\"title\" mbr-if=\"showTitle||showSubtitle\">\n\t\t\t<h3 class=\"mbr-section-title align-center mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title, .mbr-section-subtitle\">Contacts</h3>\n\t\t\t<h4 class=\"mbr-section-subtitle align-center mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-title, .mbr-section-subtitle\">\"Best shop for you and your friends\"</h4>\n\t\t</div>\n\t</div>\n\t<div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n\t\t<div class=\"mbr-row first-element\">\n\t\t\t<div class=\"map-block mbr-justify-content-center\" mbr-if=\"blockType==1 || blockType==2 || blockType==5\">\n\t\t\t\t<div class=\"google-map\" mbr-map=\"googleMap\"></div>\n\t\t\t\t<div class=\"map-placeholder\">\n\t\t\t\t\t<div placeholder>\n\t\t\t\t\t\t<div class=\"mobirise-spinner\">\n\t\t\t\t\t\t\t<em></em>\n\t\t\t\t\t\t\t<em></em>\n\t\t\t\t\t\t\t<em></em>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t\t<div class=\"contacts-block\" mbr-class=\"{'mbr-col-lg-12 mbr-col-md-12 mbr-col-sm-12': blockType==7}\" mbr-if=\"blockType==1 || blockType==3 || blockType==7\">\n\t\t\t\t<div class=\"text-block\">\n\t\t\t\t\t<div class=\"text-item\">\n\t\t\t\t\t\t<h4 class=\"mbr-section-title