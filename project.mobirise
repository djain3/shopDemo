                ".mbr-section-title": {
                  "padding-bottom": "1rem"
                }
              },
              "& when (@showSubtitle) and (@showText)": {
                ".mbr-section-subtitle": {
                  "padding-bottom": "1rem"
                }
              }
            },
            "& when (@showTitle), (@showSubtitle), (@showText)": {
              ".title-wrap": {
                "width": "100%",
                "display": "flex",
                "justify-content": "~\"@{textAlign}\"",
                ".title-block": {
                  "width": "~\"@{textWidth}%\""
                },
                "margin-bottom": "2rem",
                "@media (max-width: 992px)": {
                  "margin-bottom": "4rem"
                }
              }
            },
            ".cards-list": {
              "width": "100%",
              "display": "flex",
              "& when (@blockType = '1'), (@blockType = '3'), (@blockType = '5'), (@blockType = '7'), (@blockType = '9')": {
                "flex-direction": "column",
                "@media (min-width: 992px)": {
                  "align-items": "~\"@{cardsAlign}\""
                },
                "@media (max-width: 991px)": {
                  "align-items": "center"
                }
              },
              "& when (@blockType = '2'), (@blockType = '4'), (@blockType = '6'), (@blockType = '8')": {
                "justify-content": "center",
