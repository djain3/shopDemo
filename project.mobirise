        {
          "_styles": {
            "padding-top": "(@paddingTop * 1rem)",
            "padding-bottom": "(@paddingBottom * 1rem)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background-color": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            "& when (@showSubtitle), (@showText)": {
              "& when (@showTitle)": {
                ".mbr-section-title": {
                  "padding-bottom": "1rem"
                }
              },
              "& when (@showSubtitle) and (@showText)": {
                ".mbr-section-subtitle": {
                  "padding-bottom": "1rem"
                }
              }
            },
            "& when (@showTitle), (@showSubtitle), (@showText)": {
              ".title-wrap": {
                "width": "100%",
                "display": "flex",
                "justify-content": "~\"@{textAlign}\"",
                ".title-block": {
                  "width": "~\"@{textWidth}%\""
                },
                "margin-bottom": "2rem",
                "@media (max-width: 992px)": {
                  "margin-bottom": "4rem"
                }
              }
            },
            ".cards-list": {
              "width": "100%",
              "display": "flex",
              "& when (@blockType = '1'), (@blockType = '3'), (@blockType = '5'), (@blockType = '7'), (@blockType = '9')": {
                "flex-direction": "column",
                "@media (min-width: 992px)": {
                  "align-items": "~\"@{cardsAlign}\""
                },
                "@media (max-width: 991px)": {
                  "align-items": "center"
                }
              },
              "& when (@blockType = '2'), (@blockType = '4'), (@blockType = '6'), (@blockType = '8')": {
                "justify-content": "center",
                "flex-wrap": "wrap"
              }
            },
            ".card": {
              "position": "relative",
              "display": "flex",
              "min-width": "0",
              "word-wrap": "break-word",
              "background-clip": "border-box",
              "border-radius": "0",
              "width": "100%",
              "min-height": "1px",
              "@media (max-width: 767px)": {
                "padding": "0"
              },
              "& when (@blockType = '1'), (@blockType = '3'), (@blockType = '5')": {
                "@media (min-width: 992px)": {
                  "width": "~\"@{cardsWidth}%\""
                },
                "@media (max-width: 991px)": {
                  "width": "80%"
                },
                "@media (max-width: 767px)": {
                  "width": "100%"
                }
              },
              "& when (@blockType = '7'), (@blockType = '9')": {
                "@media (min-width: 992px)": {
                  "width": "~\"@{cardsWidth}%\""
                },
                "@media (max-width: 991px)": {
                  "width": "100%"
                }
              },
              "& when (@blockType = '9')": {
                "& when not (@reverseContent)": {
                  "@media (min-width: 768px)": {
                    "&:nth-child(even)": {
                      ".card-wrapper": {
                        "flex-direction": "row-reverse"
                      }
                    }
                  }
                },
                "& when (@reverseContent)": {
                  "@media (min-width: 768px)": {
                    "&:nth-child(odd)": {
                      ".card-wrapper": {
                        "flex-direction": "row-reverse"
                      }
                    }
                  }
                }
              }
            },
            ".card-wrapper": {
              "width": "100%",
              "height": "100%",
              "& when (@showBorders)": {
                "border": "1px solid @bordersColor"
              },
              "& when not (@transparentBg)": {
                "background": "@cardBg"
              },
              "display": "flex",
              "& when (@blockType = '3'), (@blockType = '5'), (@blockType = '7'), (@blockType = '9')": {
                "@media (max-width: 767px)": {
                  "flex-direction": "column"
                }
              },
              "& when (@blockType = '4'), (@blockType = '6'), (@blockType = '8')": {
                "flex-direction": "column"
              },
              "& when (@blockType = '7')": {
                "& when (@reverseContent)": {
                  "@media (min-width: 768px)": {
                    "flex-direction": "row-reverse"
                  }
                }
              }
            },
            "& when (@showCardsTitle)": {
              ".card-title": {
                "margin": "0"
              }
            },
            "& when (@showCardsSubtitle)": {
              ".card-subtitle": {
                "margin": "0"
              }
            },
            ".card-img": {
              "& when (@blockType = '3'), (@blockType = '5')": {
