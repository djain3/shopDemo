                "& when (@blockType = '7'), (@blockType = '9')": {
                  "@media (min-width: 992px)": {
                    "width": "~\"@{sizes}%\""
                  },
                  "@media (max-width: 991px)": {
                    "width": "30%"
                  },
                  "@media (max-width: 767px)": {
                    "width": "100%"
                  },
                  "amp-img": {
                    "height": "100%"
                  }
                },
                "& when (@blockType = '8')": {
                  "display": "flex",
                  "flex-grow": "1",
                  "width": "100%",
                  "amp-img": {
                    "flex-grow": "1"
                  }
                }
              }
            },
            ".card-box": {
              "padding": "1.5rem",
              "& when not (@blockType = '3'), (@blockType = '5')": {
                "width": "100%"
              },
              "& when (@blockType = '3'), (@blockType = '5')": {
                "@media (min-width: 768px)": {
                  "width": "calc(100% ~\"- @{sizes}%\")"
                },
                "@media (max-width: 767px)": {
                  "width": "100%"
                }
              },
              "& when (@blockType = '7'), (@blockType = '9')": {
                "display": "flex",
                "flex-direction": "column",
                "justify-content": "center",
                "@media (min-width: 992px)": {
                  "width": "calc(100% ~\"- @{sizes}%\")"
                },
                "@media (max-width: 991px)": {
                  "width": "70%"
                },
                "@media (max-width: 767px)": {
                  "width": "100%"
                }
              }
            },
