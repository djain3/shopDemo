          "_name": "team1",
          "_customHTML": "<section class=\"team1\" group=\"Teams\" plugins=\"amp-main\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"9\" step=\"1\" value=\"2\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"9\" step=\"1\" value=\"2\">\n\n        <select title=\"Block Type\" name=\"blockType\">\n            <option value=\"1\" selected>Standart Cards 1</option>\n            <option value=\"2\">Standart Cards 2</option>\n            <option value=\"3\">Cards with Body on Image 1</option>\n            <option value=\"4\">Cards with Body on Image 2</option>\n            <option value=\"5\">Cards with Icons on Image 1</option>\n            <option value=\"6\">Cards with Icons on Image 2</option>\n            <option value=\"7\">Cards with only Text 1</option>\n            <option value=\"8\">Cards with only Text 2</option>\n            <option value=\"9\">Horizontal Cards with Images 1</option>\n            <option value=\"10\">Horizontal Cards with Images 2</option>\n        </select>\n\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showSubtitle\">\n        <input type=\"range\" inline title=\"Cards Count\" name=\"cardsAmount\" min=\"1\" max=\"4\" step=\"1\" value=\"4\">\n        <input type=\"checkbox\" title=\"Rounded Image\" name=\"roundedImage\" condition=\"blockType == 1 || blockType == 2\">\n        <input type=\"range\" inline title=\"Image Width\" name=\"imageWidth\" min=\"50\" max=\"100\" step=\"1\" value=\"100\" condition=\"blockType == 1 || blockType == 2\">\n        <input type=\"color\" title=\"Image Overlay Color\" name=\"imageOverlayColor\" value=\"#ffffff\" condition=\"blockType > 2 &amp;&amp; blockType < 7\">\n        <input type=\"range\" inline title=\"Image Overlay Opacity\" name=\"imageOverlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"blockType > 2 &amp;&amp; blockType < 7\">\n        <input type=\"checkbox\" title=\"Show Name\" name=\"showName\" checked>\n        <input type=\"checkbox\" title=\"Show Specialty\" name=\"showSpecialty\">\n        <input type=\"checkbox\" title=\"Show Text\" name=\"showText\" checked>\n        <input type=\"range\" inline title=\"Icons Count\" name=\"iconsCount\" min=\"0\" max=\"4\" step=\"1\" value=\"3\" condition=\"blockType != 7 && blockType != 8\">\n        <input type=\"range\" inline title=\"Icons Size\" name=\"iconsSize\" min=\"1\" max=\"4\" step=\"0.1\" value=\"2\" condition=\"blockType != 7 && blockType != 8\">\n        <input type=\"color\" title=\"Icons Color\" name=\"iconsColor\" value=\"#66458e\" condition=\"blockType != 7 && blockType != 8\">\n        <input type=\"color\" title=\"Icons Hover BG Color\" name=\"iconsBg\" value=\"#66458e\" condition=\"blockType == 5 || blockType == 6\">\n        <input type=\"color\" title=\"Icons Hover Color\" name=\"iconsHover\" value=\"#fac769\" condition=\"blockType == 5 || blockType == 6\">\n        <input type=\"checkbox\" title=\"Image on Left/Right\" name=\"reverseContent\" condition=\"blockType > 8\">\n        <input type=\"checkbox\" title=\"Transparent Card BG\" name=\"transparentBg\" condition=\"blockType != 3 && blockType != 4\">\n        <input type=\"color\" title=\"Card BG\" name=\"cardBg\" value=\"#ffffff\" condition=\"!transparentBg && blockType != 3 && blockType != 4\">\n\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/background4.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#f4e7da\" selected>\n        </fieldset>\n        \n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" checked condition=\"bg.type!='color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"bg.type!='color' && overlay\">\n        <input type=\"range\" inline title=\"Overlay Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\" condition=\"bg.type!='color' && overlay\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\"></div>\n    <div class=\"container\">\n        <div class=\"title-wrap align-center\" mbr-if=\"showTitle || showSubtitle\">\n            <h2 class=\"mbr-section-title mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title, .underline\">\n                Team\n            </h2>\n            <h3 class=\"mbr-section-subtitle mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-subtitle\">\n                Lorem ipsum dolor sit amet, consectetur adipisicing elit.\n            </h3>\n        </div>\n        <div class=\"mbr-row mbr-justify-content-center\">\n\n            <div class=\"card mbr-col-sm-12\" mbr-class=\"{'mbr-col-md-6 mbr-col-lg-3': blockType < 9,\n                             'mbr-col-md-10 mbr-col-lg-6': blockType >= 9,\n                             'lg-pb': cardsAmount > 2 &amp;&amp; blockType >= 9,\n                             'md-pb': cardsAmount > 2 &amp;&amp; blockType < 9,\n                             'last-card': cardsAmount == 1}\">\n                <div class=\"card-wrapper\">\n                    <div class=\"card-img img-with-overlay\" mbr-if=\"blockType != 7 && blockType != 8\">\n                        <amp-img class=\"mobirise-loader\" src=\"@PROJECT_PATH@/assets/images/pexels-photo-769731-378x566.jpg\" layout=\"responsive\" width=\"189.2671232876712\" height=\"183\" alt=\"Mobirise\">\n                            <div placeholder>\n                                <div class=\"mobirise-spinner\">\n                                    <em></em>\n            