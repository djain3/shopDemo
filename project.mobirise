                "@media (max-width: 992px)": {
                  "margin-bottom": "100px",
                  "&:nth-last-child(1)": {
                    "margin-bottom": "0"
                  }
                },
                "@media (max-width: 767px)": {
                  "padding": "0 !important"
                },
                "& when (@blockType = '7'), (@blockType = '9')": {
                  ".card-title": {},
                  ".card-box": {
                    "padding": "0 3rem !important"
                  }
                }
              }
            },
            "& when (@siteStyle = Company)": {
