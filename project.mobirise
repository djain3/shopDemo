              },
              "& when not (@transparentBg)": {
                "background": "@cardBg"
              },
              "display": "flex",
              "& when (@blockType = '3'), (@blockType = '5'), (@blockType = '7'), (@blockType = '9')": {
                "@media (max-width: 767px)": {
                  "flex-direction": "column"
